msgstr "Linha %s: SKU ou preço não encontrado. Colunas disponíveis: [%s]"

#: includes/class-wcpfs-importer.php:165
msgid "Line %s: SKU is empty. Price provided: \"%s\""
msgstr "Linha %s: SKU está vazio. Preço fornecido: \"%s\""

#: includes/class-wcpfs-importer.php:173
msgid "Line %s: Price is empty. SKU: \"%s\""
msgstr "Linha %s: Preço está vazio. SKU: \"%s\""

#: includes/class-wcpfs-importer.php:182
msgid "Line %s: Invalid price \"%s\". SKU: \"%s\" - Use only numbers with a dot or comma as the decimal separator"
msgstr "Linha %s: Preço inválido \"%s\". SKU: \"%s\" - Use apenas números com ponto ou vírgula como separador decimal"

#: includes/class-wcpfs-importer.php:192
msgid "Line %s: Product not found. SKU: \"%s\" – Check if the SKU exists in WooCommerce"
msgstr "Linha %s: Produto não encontrado. SKU: \"%s\" – Verifique se o SKU existe no WooCommerce"

#: includes/class-wcpfs-importer.php:201
msgid "Line %s: Error loading product. SKU: \"%s\" – Product may be corrupted"
msgstr "Linha %s: Erro ao carregar o produto. SKU: \"%s\" – Produto pode estar corrompido"

#: includes/class-wcpfs-importer.php:220
msgid "Line %s: Sale price (%s) must be lower than the regular price (%s). SKU: \"%s\""
msgstr "Linha %s: O preço promocional (%s) deve ser menor que o preço normal (%s). SKU: \"%s\""

#: includes/class-wcpfs-importer.php:230
msgid "Line %s: Invalid sale price \"%s\". SKU: \"%s\" – Use only numbers"
msgstr "Linha %s: Preço promocional inválido \"%s\". SKU: \"%s\" – Use apenas números"

#: includes/class-wcpfs-importer.php:240
msgid "Line %s: Error saving product. SKU: \"%s\" – Error: %s"
msgstr "Linha %s: Erro ao salvar o produto. SKU: \"%s\" – Erro: %s"

